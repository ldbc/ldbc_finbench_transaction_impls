n({@Person && _id==$id}).re({@Own}).n({@Account} as account)
exta(complex_read).params({
	srcs:account,
	edgeDirection:"out",
	depth:3,
	limit:10000000,
	edge_schema:"Transfer",
	edge_timestamp:"timestamp",
	edge_amount:"amount",
    amount:-1,
    pre:1,
	range_begin:$startTime,
	range_end:$endTime,
	noCircle:1,
	top_n:$truncationLimit,
    order:$truncationOrder,
	return_type:0
}).stream() as rest
return distinct rest
order by size(rest) desc