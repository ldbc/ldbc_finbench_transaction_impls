n({@Account && is_blocked=='True'}).re({@Transfer && timestamp <> [$startTime,$endTime] && amount>$threshold}).n({@Account && _id==$id}) as path1
n({@Account}).re({@Transfer && timestamp <> [$startTime,$endTime] && amount>$threshold}).n({@Account && _id==$id}) as path2
